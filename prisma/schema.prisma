generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Product {
  id     String  @id @default(uuid())
  title  String  @db.VarChar(255) @unique
  desc   String  @db.VarChar(255)
  price  Int
}

model User {
  username String
  email    String  @db.VarChar(255) @unique
  password String
}
